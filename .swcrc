{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": false
    },
    "target": "es2016",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false,
    "baseUrl": "./lib",
    "paths": {
      "@/*": ["*"]
    },
    "preserveAllComments": false
  },
  "module": {
    "type": "commonjs",
    "strict": true,
    "strictMode": true,
    "noInterop": false,
    "lazy": false
  },
  "minify": false,
  "sourceMaps": false
}

